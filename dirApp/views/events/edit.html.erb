<%= render :partial => 'shared/menu_head', :locals => { :menu_type => 'Event' } %>

<%= content_for :content do %>
  <h2 class="grp-hdr">Edit Event</h2>
  <hr />

  <%= form_for @event, :url => event_path(@event, :cid=>@channel), :html => { :multipart => true } do |f| %>
    <%= render :partial => 'form', :locals => { :f => f, :etype => 'Event' } %>
  <% end %>
<% end %>
