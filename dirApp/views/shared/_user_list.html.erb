<table>
  <tr>
    <th></th>
    <th>Name</th>
    <th>Email</th>
    <th>Location</th>
    <th></th>
  </tr>
  <% @subscriptions.each do |sub| %>
    <% unless sub.try(:user).blank? %>
    <tr>
      <td>
        <div class="pic-size">
	  <%= render :partial => 'shared/show_photo', :locals => {:model => sub.user.profile, :file_name => "headshot1.jpg", :psize => '48x48' } %>
	</div>
      </td>
      <td><%= sub.user.name %></td>
      <td><%= sub.user.email %></td>
      <td><%= get_location(sub.user) %></td>
      <td><%= link_to "Select", select_users_url(:id=>sub.user, :ssid=>@host_profile.ssid, :cid=>@channel), :id=>'rel_id', :method => :post %></td>
    </tr>
    <% end %>
  <% end %>
</table>
<div class="nav-right">
  <%= will_paginate @subscriptions, :param_name => 'sub_page' %><br />
</div>
