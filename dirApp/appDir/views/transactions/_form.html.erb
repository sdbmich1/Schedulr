<%= form_for @transaction do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :amt %><br />
    <%= f.text_field :amt %>
  </p>
  <p>
    <%= f.label :currency %><br />
    <%= f.text_field :currency %>
  </p>
  <p>
    <%= f.label :transaction_date %><br />
    <%= f.datetime_select :transaction_date %>
  </p>
  <p>
    <%= f.label :channelID %><br />
    <%= f.text_field :channelID %>
  </p>
  <p>
    <%= f.label :HostProfileID %><br />
    <%= f.text_field :HostProfileID %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.label :hide %><br />
    <%= f.text_field :hide %>
  </p>
  <p><%= f.submit %></p>
<% end %>
