<%= render :partial => 'shared/menu_head', :locals => { :menu_type => 'Exhibitor' } %>

<%= content_for :content do %>
  <h2 class="grp-hdr">Exhibitor List</h2>
  <hr />

  <% unless @parent_event %>
    <p><%= link_to "New Exhibitor", new_event_exhibitor_url(:cid=>@channel), :id=>"link-btn", :class=>"round nav-right" %></p><br />
  <% end %>

  <p>
  <div id="pres-list"><%= render :partial => 'shared/exhibitor_list', :locals => { :event => @event, :parent => @parent_event } %></div>
  </p>
<% end %>
