<% if user_signed_in? %>
  <p><span>First Name</span> <%= f.text_field :FirstName %></p> 
  <p><span>Last Name</span> <%= f.text_field :LastName %></p> 
  <p><span>Title</span> <%= f.text_field :Title %></p> 
  <p><span>Email</span> <%= f.text_field :EMAIL %></p> 
<% end %>
<p><span>Organization*</span> <%= f.text_field :Company %></p> 
<p><span>Address*</span>  <%= f.text_field :Address1 %></p>
<p><span>Address 2</span>  <%= f.text_field :Address2 %></p>
<p><span>City*</span>  <%= f.text_field :City %></p>
<p><span>State*</span>  <%= f.collection_select :State, StateCode.all, :StateAbbr, :State, :include_blank => true %></p>
<p><span>Zip*</span>  <%= f.text_field :PostalCode %></p>
<p><span>Org Type*</span><%= f.collection_select :EntityType, EntityType.all, :code, :description, :prompt => 'Select' %></p>
<% unless user_signed_in? %>
  <p><span>Promo Code</span>  <%= f.text_field :promoCode %></p>
<% end %>
